<br>
<h3 style="text-align: center;">Attendance Table</h3>

<table border="1" style="width:100%; margin-top: 20px;">
  <thead>
    <tr>
      <th>Date</th>
      <th>Attendance</th>
      <th>Remarks</th>
      <th>Courses</th>
    </tr>
  </thead>
  <tbody>
    {{#attendanceData}}
      <tr>
        <td>{{date}}</td>
        <td>{{attendance}}</td>
        <td>{{remarks}}</td>
        <td>{{courses}}</td>
      </tr>
    {{/attendanceData}}
  </tbody>
</table>

<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="local/control/db" VERSION="20200213" COMMENT="XMLDB file for local control plugin"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="parents_login" COMMENT="Each record is of parents login credentials">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="username" TYPE="text" NOTNULL="true" SEQUENCE="false" />
        <FIELD NAME="password" TYPE="text" NOTNULL="true" SEQUENCE="false" />
        <FIELD NAME="full_name" TYPE="text" NOTNULL="true" SEQUENCE="false" />
        <FIELD NAME="student_id" TYPE="text" NOTNULL="true" SEQUENCE="false" />
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>
    <TABLE NAME="results_of_students" COMMENT="Each record is of records of students">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="subjects" TYPE="text" NOTNULL="true" SEQUENCE="false" />
        <FIELD NAME="marks_obtained" TYPE="text" NOTNULL="true" SEQUENCE="false" />
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>

     <TABLE NAME="students_data" COMMENT="Each record is of student information">
  <FIELDS>

       
    <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
    <FIELD NAME="name" TYPE="text" NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="dept_name" TYPE="text" NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="tot_cred" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" UNSIGNED="true" />

    <!-- Remove other fields -->

  </FIELDS>
  <KEYS>
    <KEY NAME="primary" TYPE="primary" FIELDS="id" />
  </KEYS>
</TABLE>

<TABLE NAME="courses_data" COMMENT="Each record provides courses information">
  <FIELDS>
        <FIELD NAME="uselessid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
    <FIELD NAME="courseid" TYPE="text" LENGTH="10" NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="coursename" TYPE="text" NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="dept_name" TYPE="text" NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="credits" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" UNSIGNED="true" />

    <!-- Remove other fields -->

  </FIELDS>
  <KEYS>
    <KEY NAME="primary" TYPE="primary" FIELDS="uselessid" />
  </KEYS>
</TABLE>

<TABLE NAME="takes" COMMENT="Each record provides courses information">
  <FIELDS>
        <FIELD NAME="uselessid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />

    <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="courseid" TYPE="text"  NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="semester" TYPE="text" NOTNULL="true" SEQUENCE="false" />
    <FIELD NAME="year" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" />
    <FIELD NAME="grade" TYPE="text" NOTNULL="true" SEQUENCE="false" />

    <!-- Remove other fields -->

  </FIELDS>
  <KEYS>
    <KEY NAME="primary" TYPE="primary" FIELDS="uselessid" />
  </KEYS>
</TABLE>








  </TABLES>
</XMLDB>